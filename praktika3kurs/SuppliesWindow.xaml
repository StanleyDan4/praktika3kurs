<Window x:Class="praktika3kurs.SuppliesWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:praktika3kurs"
        mc:Ignorable="d"
        Title="SuppliesWindow" Height="450" Width="800">
    <Grid>
        <DataGrid Name="SuppliesDataGrid" AutoGenerateColumns="False" IsReadOnly="True" CanUserAddRows="False" Margin="10">
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID" Binding="{Binding Id}" Width="50"/>
                <DataGridTextColumn Header="Price" Binding="{Binding Price}" Width="*"/>
                <DataGridTextColumn Header="Agent ID" Binding="{Binding AgentId}" Width="*"/>
                <DataGridTextColumn Header="Client ID" Binding="{Binding ClientId}" Width="*"/>
                <DataGridTextColumn Header="Real Estate ID" Binding="{Binding RealEstateId}" Width="*"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
