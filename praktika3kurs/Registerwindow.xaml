<Window x:Class="praktika3kurs.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:praktika3kurs"
        mc:Ignorable="d"
        Title="RegisterWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Top" Margin="10">
            <Button Content="Agents" Click="ShowAgentsButton_Click" Margin="5"/>
            <Button Content="Apartments" Click="ShowApartmentsButton_Click" Margin="5"/>
            <Button Content="Demands" Click="ShowDemandsButton_Click" Margin="5"/>
            <Button Content="Supplies" Click="ShowSuppliesButton_Click" Margin="5"/>
        </StackPanel>
    </Grid>
</Window>
